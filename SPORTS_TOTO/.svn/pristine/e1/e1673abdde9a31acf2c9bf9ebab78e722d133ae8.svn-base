<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=EUC-KR">
	<link rel="stylesheet" type="text/css" href="../css/table.css?ver=6">
	<script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
	<title>Insert title here</title>
</head>
<body>
	<center>
		<form action="newmember_ok.jsp" method="post">
			<table id="table_member2" border="1" width="400" height="600" cellspacing="0">
				<tr>
					<td colspan="2" align="center" style="border-top-left-radius: 1em;border-top-right-radius: 1em;">
						<h1>회원가입</h1>
					</td>
				</tr>
				<tr>
					<th width="30%">이름</th>
					<td width="70%"><input type="text" name="name"></td>
				</tr>
				<tr>
					<th width="30%">아이디</th>
					<td width="70%"><input type="text" name="id"></td>
				</tr>
				<tr>
					<th width="30%">비밀번호</th>
					<td width="70%"><input type="password" name="pwd"></td>
				</tr>
				<tr>
					<th width="30%">성별</th>
					<td width="70%">
						<input type="radio" name="gender" value="남자" checked="checked">남자
						<input type="radio" name="gender" value="여자">여자
					</td>
				</tr>
				<tr>
					<th width="30%">생일</th>
					<td width="70%"><jsp:include page="calendar.jsp"></jsp:include></td>
				</tr>
				<tr>
					<th width="30%">이메일</th>
					<td width="70%"><input type="text" name="email"></td>
				</tr>
				<tr>
					<th width="30%">주소</th>
					<td width="70%"><input type="text" name="address"></td>
				</tr>
				<tr>
					<th width="30%">핸드폰번호</th>
					<td width="70%"><input type="text" name="phone"></td>
				</tr>
				<tr>
					<td colspan="2" align="center" style="border-bottom-left-radius: 1em;border-bottom-right-radius: 1em;">
						<input type="submit" value="회원 가입">
						<input type="button" value="취소" onclick="self.close();">
					</td>
				</tr>
			</table>
		</form>
	</center>
</body>
</html>
